# Neynar API Configuration
# Sign up at https://neynar.com to get your API key
NEYNAR_API_KEY=your_neynar_api_key_here

# Farcaster Bot Configuration
# The Signer UUID from Neynar's dedicated signer setup process
# Follow instructions at: https://docs.neynar.com/docs/farcaster-bot-with-dedicated-signers
SIGNER_UUID=your_neynar_signer_uuid_here

# Your bot's Farcaster ID (FID)
# This is the numeric ID of your bot's Farcaster account
BOT_FID=12345

# The username of your bot without the @ symbol
# This is used when checking for mentions in text
BOT_NAME=zoiner

# Wallet Configuration for Zora Token Creation
# Private key of the wallet that will deploy tokens (keep this secure!)
# Must have funds on the Base network
WALLET_PRIVATE_KEY=

# RPC Provider URL for Base network
# You can use the default or get a dedicated endpoint from providers like Alchemy
RPC_URL=https://mainnet.base.org

# Server Configuration
# Port the webhook server will run on
PORT=3000 

PINATA_API_KEY=